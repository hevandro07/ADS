#include <stdio.h>
#include <locale.h>
int main(){
	setlocale (LC_ALL,"");
	int num, maior, menor;
	printf("Digite algum número:\n");
	scanf("%d",&num);
	maior=num;
	menor=num;
	
    printf("Digite algum número:\n");  
	scanf("%d",&num);
	if(num > maior)
		maior=num;
	if(num < menor)
		menor=num;
		
	printf("Digite algum número:\n");  
	scanf("%d",&num);
	if(num > maior)
		maior=num;
	if(num < menor)
		menor=num;
		
	printf("Digite algum número:\n");  
	scanf("%d",&num);
	if(num > maior)
		maior=num;
	if(num < menor)
		menor=num;
		
    printf("Digite algum número:\n");  
	scanf("%d",&num);
	if(num > maior)
		maior=num;
	if(num < menor)
		menor=num;
	
	printf("o numero maior é:%d,e o menor é:%d.",maior,menor);
	return (0);
}
